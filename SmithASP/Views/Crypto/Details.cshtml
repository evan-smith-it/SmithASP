@model SmithASP.Models.Cryptogram

@{
    ViewData["Title"] = "Details";
}
<div>
    <h4>Cryptogram</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="message" class="col-sm-2 col-form-label">Message: </label>
        </dt>
        <dd class="col-sm-10">
            <input asp-for="message" class="form-control" readonly />
        </dd>
        <dt class="col-sm-2">
            <label asp-for="result" class="col-sm-2 col-form-label">Result: </label>
        </dt>
        <dd class="col-sm-10">
            <input asp-for="result" class="form-control" readonly />
        </dd>
        <dt class="col-sm-2">
            <label asp-for="key" class="col-sm-2 col-form-label">Key: </label>
        </dt>
        <dd class="col-sm-10">
            <input asp-for="key" class="form-control" readonly />
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.CryptogramId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
